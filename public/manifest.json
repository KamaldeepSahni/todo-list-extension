{
  "manifest_version": 2,
  "short_name": "Todo List",
  "name": "Todo List",
  "description": "TodoList chrome extension",
  "icons": { "128": "images/icon_128.png" },
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "/images/icon.png",
    "default_popup": "index.html",
    "default_title": "Todo List"
